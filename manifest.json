  "manifest_version": 2,

  "name": "PointRoll PageBoy",
  "description": "This extension provides information on PointRoll ads.",
  "version": "0.9",

  "browser_action": {
    "default_icon": "icon.PNG",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
	"http://*/*"
  ]
}
